#include<stdio.h>
#include<stdlib.h>
#include<string.h>

void main()
{
    char s[20];
    int i,j,count,k;
    printf("Enter a string");
    scanf("%s",s);
    k=strlen(s);
while(1)
{    
for(i=0;i<=k;i++)
    {
        count=0;
        if(s[i]!='\0')
        {
            for(j=0;i<=k;j++)
            {
                if(s[i]==s[j])
                {
                    count++;
                    s[j]='\0';
                }
            }
            printf("freq of %c is %d",s[i],count);
    	}
    }
 printf("Enter another string");
    scanf("%a",s);
    k=strlen(a);
    for(i=0;i<=k;i++)
    {
        count=0;
        if(a[i]!='\0')
        {
            for(j=0;i<=k;j++)
            {
                if(a[i]==s[j])
                {
                    count++;
                    a[j]='\0';
                }
            }
            printf("freq of %c is %d",s[i],count);
    
        }
    }


    
}
